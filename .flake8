[flake8]
# Flake 8 linting configuration, not supported in pyproject.toml

ignore = E203,W503
max-line-length = 120
exclude =
  .svn,
  CVS,
  .bzr,
  .hg,
  .git,
  __pycache__,
  .tox
  .venv/
  venv/
  .eggs/
  build/
